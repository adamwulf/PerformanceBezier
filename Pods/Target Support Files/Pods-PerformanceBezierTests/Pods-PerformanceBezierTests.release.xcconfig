OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/JRSwizzle" -isystem "${PODS_ROOT}/Headers/Public/PerformanceBezier"
OTHER_LDFLAGS = -ObjC -ObjC++ -l"Pods-PerformanceBezierTests-JRSwizzle" -l"Pods-PerformanceBezierTests-PerformanceBezier" -l"stdc++" -framework "Foundation" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods